<h1>Listado de todos los articulos</h1>
 
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>
 
  <% @all_productos.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.text %></td>
    </tr>
  <% end %>
</table>
<button  type="button" class="btn btn-success"><a href="/productos/new">Click Me!</a></button>

